<el>
	<exclusivesTags>
		<TCEforms>
			<label>LLL:EXT:ics_gmap3/locallang.xml:provider.tagList.exclusivesTags</label>	
			<config>
				<type>text</type>
				<cols>50</cols>
				<rows>30</rows>
			</config>
		</TCEforms>
	</exclusivesTags>
	<hiddenTags>
		<TCEforms>
			<label>LLL:EXT:ics_gmap3/locallang.xml:provider.tagList.hiddenTags</label>	
			<config>
				<type>text</type>
				<cols>50</cols>
				<rows>30</rows>
			</config>
		</TCEforms>
	</hiddenTags>
	<defaultTags>
		<TCEforms>
			<label>LLL:EXT:ics_gmap3/locallang.xml:provider.tagList.defaultTags</label>	
			<config>
				<type>text</type>
				<cols>50</cols>
				<rows>30</rows>
			</config>
		</TCEforms>
	</defaultTags>
	<defaultMapEmpty>
		<TCEforms>
			<label>LLL:EXT:ics_gmap3/locallang.xml:provider.tagList.defaultMapEmpty</label>	
			<config>
				 <type>check</type>
                <default>0</default>
			</config>
		</TCEforms>
	</defaultMapEmpty>
	<checkOnParent>
		<TCEforms>
			<label>LLL:EXT:ics_gmap3_hierarchical_taglist/locallang.xml:provider.tagList.checkOnParent</label>	
			<config>
				 <type>check</type>
                <default>0</default>
			</config>
		</TCEforms>
	</checkOnParent>
	<separator>
		<TCEforms>
		 <exclude>1</exclude>
		 <label>LLL:EXT:ics_gmap3_hierarchical_taglist/locallang.xml:provider.tagList.separator</label>
			<config>
				<type>input</type>
				<size>48</size>
				<eval>trim</eval>
				<default></default>
			</config>
		</TCEforms>
	</separator>
</el>